<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.BorderPane?>

<ScrollPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="desktop.AppController">
   <content>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
         <center>
            <TabPane prefHeight="100.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
              <tabs>
                <Tab fx:id="detailsTab" text="Details">
                     <content>
                        <fx:include fx:id="detailsComponent" source="details/details.fxml" />
                     </content>
                  </Tab>
                <Tab fx:id="newExecutionTab" text="New Execution">
                     <content>
                        <fx:include fx:id="executionComponent" source="execution/execution.fxml" />
                     </content></Tab>
                  <Tab fx:id="resultsTab" text="Results">
                     <content>
                        <fx:include fx:id="resultsComponent" source="results/results.fxml" />
                     </content>
                  </Tab>
              </tabs>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </TabPane>
         </center>
         <top>
            <fx:include fx:id="headerComponent" source="header/header.fxml" />
         </top>
      </BorderPane>
   </content>
</ScrollPane>
