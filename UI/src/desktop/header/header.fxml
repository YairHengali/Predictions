<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="84.0" prefWidth="530.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="desktop.header.HeaderController">
   <children>
      <Label text="Predictions" />
      <HBox prefWidth="200.0">
         <children>
            <Button fx:id="loadFileBTN" mnemonicParsing="false" onAction="#loadFileButtonActionListener" text="Load XML File" />
            <TextField fx:id="loadFileTF" editable="false" minWidth="300.0" prefHeight="25.0" text="No file Loaded">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin></TextField>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
